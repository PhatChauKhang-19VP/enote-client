<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pck.enote.controller.MediaViewController">
    <MediaView fx:id="mvVideo" fitHeight="320.0" fitWidth="480.0">
        <viewport>
          <Rectangle2D />
        </viewport>
    </MediaView>
    <Button fx:id="btnPlay" layoutX="205.0" layoutY="330.0" mnemonicParsing="false" onAction="#play" prefWidth="70.0" text="Play" />
    <Button fx:id="btnPause" layoutX="110.0" layoutY="330.0" mnemonicParsing="false" onAction="#pause" prefWidth="70.0" text="Pause" />
    <Button fx:id="btnReset" layoutX="300.0" layoutY="330.0" mnemonicParsing="false" onAction="#reset" prefWidth="70.0" text="Reset" />
</AnchorPane>
